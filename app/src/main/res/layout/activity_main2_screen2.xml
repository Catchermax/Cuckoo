<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main2_screen2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.maximfursov.cuckoo.Main2ActivityScreen2">

    <ImageButton
        android:src="@drawable/ic_video"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        android:id="@+id/imageButton_ic_video"
        android:layout_below="@+id/imageButton_ic_camera"
        android:layout_alignStart="@+id/imageButton_ic_camera"
        android:layout_marginTop="41dp"
        android:background="@null"
        tools:ignore="ContentDescription"
        android:onClick="click_imageButton_ic_video"/>

    <ImageButton
        android:src="@drawable/ic_video_player"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        android:id="@+id/imageButton_ic_video_player"
        android:layout_marginStart="34dp"
        android:layout_alignTop="@+id/imageButton_ic_video"
        android:layout_toEndOf="@+id/imageButton_ic_video"
        android:background="@null"
        tools:ignore="ContentDescription"/>

    <ImageButton
        android:src="@drawable/ic_landscape"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        android:id="@+id/imageButton_ic_landscape"
        android:layout_alignTop="@+id/imageButton_ic_camera"
        android:layout_alignStart="@+id/imageButton_ic_video_player"
        android:background="@null"
        tools:ignore="ContentDescription"/>

    <ImageButton
        android:src="@drawable/ic_camera"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        android:id="@+id/imageButton_ic_camera"
        android:layout_marginTop="39dp"
        android:layout_marginStart="38dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="@null"
        tools:ignore="ContentDescription"
        android:onClick="click_imageButton_ic_camera"/>

    <ImageButton
        android:src="@drawable/ic_customer_service"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        android:id="@+id/imageButton_ic_customer_service"
        android:layout_below="@+id/imageButton_ic_video"
        android:layout_alignStart="@+id/imageButton_ic_video"
        android:layout_marginTop="44dp"
        android:background="@null"
        tools:ignore="ContentDescription"/>

    <ImageButton
        android:src="@drawable/ic_microphone"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:scaleType="centerCrop"
        android:id="@+id/imageButton_ic_microphone"
        android:layout_alignTop="@+id/imageButton_ic_customer_service"
        android:layout_alignStart="@+id/imageButton_ic_video_player"
        android:background="@null"
        tools:ignore="ContentDescription"/>
</RelativeLayout>
